{
  "name": "iridium",
  "displayName": "Iridium",
  "description": "Nest.js Client",
  "private": true,
  "version": "0.0.1",
  "workspaces": [
    "extensions/*",
    "ui/*"
  ],
  "scripts": {
    "vscode:prepublish": "tsc --build",
    "dev:database": "concurrently \"yarn --cwd extensions\/database dev\" \"yarn --cwd ui\/database dev\""
  },
  "dependencies": {
    "concurrently": "^5.3.0",
    "typescript": "^4.1.3"
  }
}
