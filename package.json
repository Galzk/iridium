{
  "name": "iridium",
  "displayName": "Iridium",
  "description": "Nest.js Client",
  "private": true,
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:iridium.view"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "iridium.view",
          "name": "Iridium"
        }
      ]
    },
    "commands": []
  },
  "workspaces": ["packages/*"],
  "scripts": {
    "vscode:prepublish": "yarn --cwd packages/extension run compile",
    "watch": "yarn --cwd packages/extension watch"
  }
}
