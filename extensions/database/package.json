{
  "name": "iridium.database",
  "version": "0.1.0",
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run build",
    "build": "tsc",
    "dev": "tsc --build --watch",
    "pretest": "yarn run build && yarn run lint",
    "lint": "eslint . --ext ts"
  },
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:iridium.database"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "iridium",
          "title": "Iridium",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "iridium": [
        {
          "type": "webview",
          "id": "iridium.database",
          "name": "Database"
        }
      ]
    },
    "commands": []
  },
  "dependencies": {
    "@babel/generator": "^7.12.11",
    "@babel/parser": "^7.12.11",
    "@babel/traverse": "^7.12.12",
    "@babel/types": "^7.12.12",
    "@types/uuid": "^8.3.0",
    "axios": "^0.21.1",
    "concurrently": "^5.3.0",
    "inversify": "^5.0.5",
    "knex": "^0.21.16",
    "knex-schema-inspector": "^0.0.25",
    "mysql": "^2.18.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.2",
    "@types/babel__traverse": "^7.11.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  }
}
